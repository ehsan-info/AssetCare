    "classnames": "^2.3.1",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.8.0",
    "list.js": "2.3.1",
    "moment": "^2.29.3",
    "normalizr": "^3.6.1",
    "nouislider": "15.5.1",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-bootstrap-sweetalert": "5.2.0",
    "react-datetime": "^3.1.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.3.1",
    "react-leaflet": "^4.0.0",
    "react-loader-spinner": "^5.1.4",
    "react-pdf": "^5.7.1",
    "react-perfect-scrollbar": "1.5.8",
    "react-rating": "^2.0.5",
    "react-router": "6.2.2",
    "react-router-dom": "6.2.2",
    "react-scripts": "5.0.1",
    "react-select": "^5.2.2",
    "react-select2-wrapper": "^1.0.4-beta6",
    "react-table": "^7.7.0",
    "react-uuid": "^1.0.2",
    "react-vis": "^1.11.7",
